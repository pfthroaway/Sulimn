        <Button x:Name="btnManageUsers" Width="125" Height="30" Content="Manage _Users" Margin="10" Click="btnManageUsers_Click" />
        <Button x:Name="btnBack" Width="125" Height="30" Content="_Back" Margin="10" Click="btnBack_Click" IsCancel="True" />
    </StackPanel>
</Window>